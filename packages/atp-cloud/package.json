{
  "name": "@atp/cloud",
  "version": "0.1.0-alpha",
  "private": true,
  "type": "module",
  "description": "ATP Cloud - Multi-tenant Agent Trust Protocol Cloud Platform (INTERNAL TESTING ONLY)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": ["agent-trust-protocol", "multi-tenant", "cloud", "internal-testing"],
  "scripts": {
    "build": "npm run build:services && npm run build:dashboard",
    "build:services": "tsc -p tsconfig.services.json",
    "build:dashboard": "cd dashboard && npm run build",
    "dev": "npm run dev:services & npm run dev:dashboard",
    "dev:services": "tsc -p tsconfig.services.json --watch",
    "dev:dashboard": "cd dashboard && npm run dev",
    "dev:gateway": "node --loader ts-node/esm src/cloud-gateway/index.ts",
    "dev:tenant": "node --loader ts-node/esm src/tenant-service/index.ts",
    "dev:analytics": "node --loader ts-node/esm src/analytics-service/index.ts",
    "test": "jest --passWithNoTests",
    "test:services": "jest src --passWithNoTests",
    "test:dashboard": "cd dashboard && npm test",
    "lint": "eslint src --ext .ts && cd dashboard && npm run lint",
    "clean": "rm -rf dist && rm -rf dashboard/dist && rm -rf dashboard/.next",
    "docker:build": "docker-compose -f docker/docker-compose.yml build",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "cloud:deploy": "scripts/deploy-cloud.sh",
    "cloud:status": "scripts/check-deployment.sh"
  },
  "dependencies": {
    "express": "^4.18.0",
    "ws": "^8.13.0",
    "http-proxy-middleware": "^2.0.0",
    "cors": "^2.8.5",
    "winston": "^3.8.0",
    "zod": "^3.21.0",
    "@noble/ed25519": "^2.0.0",
    "@noble/hashes": "^1.3.0",
    "@atp/shared": "^0.1.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "jose": "^5.0.0",
    "redis": "^4.6.0",
    "mongodb": "^6.0.0",
    "stripe": "^14.0.0",
    "node-cron": "^3.0.0",
    "helmet": "^7.0.0",
    "rate-limiter-flexible": "^5.0.0",
    "nanoid": "^5.0.0",
    "bcrypt": "^5.1.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/ws": "^8.5.0",
    "@types/cors": "^2.8.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agent-trust-protocol/atp-cloud.git",
    "directory": "packages/atp-cloud"
  }
}